se}}},{"text_run":{"content":"$","text_element_style":{"bold":true,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":"             //光标移到文本的行尾\nA             //光标移到文本的行尾并切换到编辑模式\n^             //光标移到文本的行首\nG             //光标移到文本的末尾\ngg            //光标移到文本的首行\nZZ            //存盘退出\n/字符串        //查找某个字符串\nn             //继续查找\n:u            //撤销\n","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}},{"text_run":{"content":":redo         //重做","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"language":7,"wrap":false}},"parent_id":"DNKCdG8xYoUq1Lxu1l1cIXFWnsb"},{"block_id":"N1X2dz9IYolHIbxtJCXcmIrWndb","block_type":2,"parent_id":"DNKCdG8xYoUq1Lxu1l1cIXFWnsb","text":{"elements":[{"text_run":{"content":"","text_element_style":{"bold":false,"inline_code":false,"italic":false,"strikethrough":false,"underline":false}}}],"style":{"align":1,"folded":false}}}],"type":"docx"}